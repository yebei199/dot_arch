{
    "closeOnFocusLoss": false,
    "faviconService": "twenty",
    "font": {
        "size": 12,
        "family": "Cantarell"
    },
    "keybinding": "default",
    "keybinds": {},
    "popToRootOnClose": false,
    "rootSearch": {
        "searchFiles": false
    },
    "theme": {
        "background": "#1e1e2e",
        "foreground": "#c0caf5",
        "highlight": "#7aa2f7",
        "accent": "#f7768e"
    },
    "window": {
        "csd": true,
        "opacity": 0.75,
        "rounding": 12,
        "shadow": true,
        "padding": 8
    },
    "commands": [
        {
            "name": "QQ",
            "command": "linuxqq --ozone-platform-hint=auto --enable-wayland-ime --enable-features=UseOzonePlatform --wayland-text-input-version=3"
        },
        {
            "name": "Feishu",
            "command": "feishu --ozone-platform-hint=auto --enable-wayland-ime --enable-features=UseOzonePlatform --wayland-text-input-version=3"
        }
    ]
}
